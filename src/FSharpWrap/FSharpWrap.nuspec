<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <authors>davnavr</authors>
    <description>
      Generates F# modules and functions from non-F# dependencies.
    </description>
    <id>FSharpWrap</id>
    <releaseNotes>$PackageReleaseNotes$</releaseNotes>
    <repository type="git" url="https://github.com/davnavr/FSharpWrap" />
    <version>$PackageVersion$</version>
  </metadata>
  <files>
    <file src="tool\*.*" target="build" />
    <file src="FSharpWrap.*" exclude="*.nuspec" target="build" />
  </files>
</package>
